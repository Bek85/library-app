{{page-title 'Seeder'}}
<h1>
  Seeder, our Data Center
</h1>
<div class="row">
  <div class="col-md-4">
    {{log this.model}}
    <NumberBox @title="Libraries" @number={{this.libraries.length}} />
  </div>
  <div class="col-md-4">
    <NumberBox @title="Authors" @number={{this.authors.length}} />
  </div>
  <div class="col-md-4">
    <NumberBox @title="Books" @number={{this.books.length}} />
  </div>
</div>
{{outlet}}
